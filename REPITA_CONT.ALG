Algoritmo "Repita_cont"

Var
     a,b: inteiro


Inicio
      escreva("Digite até quanto você quer contar: ")
      leia(a)
      escreva("Qual o número inicial da contagem: ")
      leia(b)
      repita
            escreval(b)
            b <- b + 1
      Ate (b > a)
      escreval("Você chegou ao final da contagem")


Fimalgoritmo